<?xml version="1.0" ?>
<DynamicsSystem 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="SceneFile"  
    xsi:schemaLocation="SceneFile SceneFile.xsd">

<SceneSettings>

    <Visualization>
        <SceneScale value="20"/> 
    </Visualization>
    
    <RecorderSettings mode="everyTimeStep" statesPerSecond="200"/>
    
    <TimeStepperSettings deltaT ="0.001" endTime="10">
        <InclusionSolverSettings        method="SORContact" 
                                        useGPU ="false" 
                                        useGPUID="0" 
                                        alphaJORProx="0.2" 
                                        alphaSORProx="1.5" 
                                        matrixRStrategy="max"
                                        computeResidual="true"
                                        convergenceMethod="InVelocity"
                                        minIter="50" maxIter="1000" 
                                        absTol="1e-6" relTol="1e-6" 
                                        isFiniteCheck = "false"/>

<!--
        <SimulateFromReference enabled="false" type="continue" file="./SimFiles/5000Balls/SimulationState.sim" />
-->
<!--
        <SimulateFromReference type="continue" file="./SimFiles/Reference/StateData.sim" enabled="true" />
-->
    </TimeStepperSettings>
     
    <ContactParameterMap>
        <ContactParameterStandard type="UCF" mu="0.5" epsilonN="0.5" epsilonT="0.0"/>
        <ContactParameter materialId1="1" materialId2="2" type="UCF"  mu="0.3" epsilonN="0.4" epsilonT="0" invDampingN="0.0" invDampingT=".5"/>
        <ContactParameter materialId1="1" materialId2="1" type="UCF"  mu="0.3" epsilonN="0.4" epsilonT="0" invDampingN="0.0" invDampingT=".5"/>
    </ContactParameterMap>
    
    <GlobalGeometries>
        <Sphere distribute="random" radius="0.08" seed="5" minRadius="0.03" maxRadius="0.05" id="1" instances="10" />
    </GlobalGeometries>
    
    <ExternalForces>
<!--
        <ForceField  applyTo="all" type="spatialspherical-timerandom" seed="0" boostTime="0.8" pauseTime="1" amplitude="0.5" randomOn="true" minPoint="-6 -6 -2" maxPoint="6 6 5"  startTime="0" endTime="1000" />
-->
        <ForceField  applyTo="all" type="gravity" value="10" direction="0 0 -1" />
    </ExternalForces>
        

<!--
    <GlobalInitialCondition file="./GlobalFolderD/SimFiles/SimDataRECORDMPI_3/SimState.sim" whichState="end" time="2.0499" useTimeToContinue="true"/>
-->
    


<!--
    <GlobalSelectiveIds>
        <Set value="0,0  0,9 0,3" />
    </GlobalSelectiveIds>
-->


    
    <MPISettings>
        <ProcessTopology type="grid"  dimension="14 14 1" mode="static" minPoint="-3.02 -3.02 -0.02" maxPoint="3.02 3.02 3.02">
            <Visualization>
                <Material name="PlaneYellowWireNoCull"/>    
            </Visualization>   
        </ProcessTopology>
        
        <InclusionSolverSettings  splitNodeUpdateRatio="1" convergenceCheckRatio="1"/>
    </MPISettings>
    
</SceneSettings>




<SceneObjects>



    <RigidBodies name="balls1" instances="1000" groupId="0" enableSelectiveIds="true" >
        
<!--
        <Geometry>
            <Sphere distribute="random" radius="0.055" seed="5" minRadius="1.1" maxRadius="1.3" />
        </Geometry>
-->
        <Geometry>
            <GlobalGeomId distribute="random" id="1" seed="1" startId = "1" endId="10" />
        </Geometry>
        
        <DynamicProperties>
            <DynamicState type="simulated" />
            <Mass distribute="uniform" value="1" />
            <InertiaTensor type="homogen" />
            <Material distribute="uniform" id="1"/>
            <InitialCondition type="posvel">
                <InitialPosition distribute="grid"  gridSizeX="30" gridSizeY="30" distance="0.12001" translation="0.00 0.00 1.8" jitter="true"  seed="4" delta="0.02"/>

                <InitialVelocity distribute="transrot">
                    <Vel transDir="0 0 -1" absTransVel="1" rotDir="1 0 0" absRotVel="0"/>
                </InitialVelocity>
         
            </InitialCondition>
        </DynamicProperties>

        <Visualization>
                <Mesh file="Sphere18x18.mesh" scaleLikeGeometry="true" scale="0.0125 0.0125 0.0125" type="permutate" >
                    <Material name="SphereRed" />
                    <Material name="SphereYellow" />
                    <Material name="SphereBlue" />
                </Mesh>
        </Visualization>

    </RigidBodies>
    
    
